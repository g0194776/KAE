<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="CustomerConfig" type="KJFramework.Configurations.CustomerConfig, KJFramework"/>
  </configSections>
  <CustomerConfig>
    <Logger>
      <ConfigurationSettings LogPath="D:\Logs\KJFramework-Message-BaseLayer\"/>
    </Logger>
